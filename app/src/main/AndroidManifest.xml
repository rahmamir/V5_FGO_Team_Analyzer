<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.localmirza.v5_fgo_team_analyzer">

    <uses-permission android:name="android.permission.INTERNET" />

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".ViewCharecters" />
        <activity android:name=".NewFormation" />
        <activity android:name=".LoadFormation" />
        <activity android:name=".ScoreActivity" />
        <activity android:name=".BattleEmulationActivity" />
        <activity android:name=".MaterialsUpgradeCostActivity" />
        <activity android:name=".ViewServantDetails" />
        <activity android:name=".ViewCraftEssences" />
        <activity android:name=".MainMenuActivity" />
        <receiver android:name=".NotificationActivity">
            <intent-filter>
                <action android:name="DISPLAY_NOTIFICATION"/>
            </intent-filter>
        </receiver>
    </application>

</manifest>